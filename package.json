{
  "name": "smartphoto",
  "version": "0.2.0",
  "description": "smartphoto",
  "homepage": "http://developer.a-blogcms.jp",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:js": "npm-run-all -p build:lib build:jquery build:global",
    "build:lib": "npm run babel",
    "build:jquery": "browserify ./src/adaptor/jquery.js -t babelify -t [ stringify --extensions [.html .css] ] -p licensify -o ./js/jquery-smartphoto.js",
    "build:global": "browserify ./src/index.js -t babelify -t [stringify --extensions [.html .css] ] -p licensify --standalone smartPhoto -o ./js/smartphoto.js",
    "babel": "babel src --out-dir lib",
    "sass": "node-sass ./scss/smartphoto.scss ./css/smartphoto.min.css --output-style compressed",
    "watch:js": "onchange \"src/\" -- npm run build:js",
    "watch:sass": "onchange \"scss\" -- npm run sass",
    "sync": "browser-sync start --server './' --files './js/*.js' './css/*.css' --startPath '/test/jquery.html'",
    "start": "npm-run-all -p watch:js watch:sass sync"
  },
  "author": "appleple",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-html-import-to-string": "^0.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.6",
    "browserify": "^13.1.1",
    "http-server": "^0.9.0",
    "licensify": "^3.1.2",
    "node-sass": "^4.3.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "onchange": "^3.2.1",
    "stringify": "^5.1.0",
    "uglify-js": "^2.7.5",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "a-template": "0.1.4",
    "keyboard-js": "^1.1.4",
    "zepto-browserify": "^1.1.6-1"
  }
}
